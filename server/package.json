{
  "name": "miso-test",
  "version": "1.1.0",
  "description": "Production-ready demonstration application showcasing all DataClient capabilities",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.json src/server.ts",
    "build": "pnpm run build:server && pnpm run build:frontend",
    "build:server": "tsc",
    "build:frontend": "bash scripts/build-frontend.sh",
    "watch:frontend": "bash frontend/scripts/watch-build.sh",
    "start": "node dist/src/server.js",
    "start:dev": "bash scripts/start-dev.sh",
    "kill-port": "bash scripts/kill-port.sh",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "node --unhandled-rejections=warn -r ../tests/setup-rejection-handler.js node_modules/jest/bin/jest.js --maxWorkers=100% --no-coverage",
    "test:coverage": "node --unhandled-rejections=warn -r ../tests/setup-rejection-handler.js node_modules/jest/bin/jest.js --coverage",
    "test:stubs": "node frontend/scripts/test-stubs.cjs",
    "test:before-browser": "node frontend/scripts/test-before-browser.cjs",
    "patch:ioredis": "node frontend/scripts/patch-ioredis.cjs",
    "postinstall": "node frontend/scripts/patch-ioredis.cjs",
    "validate": "npm run format:check && npm run lint && npm run test && npm run test:coverage"
  },
  "keywords": [
    "aifabrix",
    "miso",
    "dataclient",
    "demo"
  ],
  "author": "AI Fabrix Team",
  "license": "MIT",
  "dependencies": {
    "@aifabrix/miso-client": "file:..",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.22.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "esbuild": "^0.19.12",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "6.3.5",
    "vitest": "^2.1.9"
  }
}

# AI Fabrix Miso Client Configuration
# Copy this file to .env and update with your values

# Required: Controller and Client Credentials
MISO_CONTROLLER_URL=http://localhost:3000
MISO_CLIENTID=ctrl-dev-my-app
MISO_CLIENTSECRET=your-client-secret-here

# Optional: Redis caching (recommended)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_KEY_PREFIX=miso:

# Optional: Logging configuration
MISO_LOG_LEVEL=info
